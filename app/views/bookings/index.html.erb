<div class="container">
  <h1>My Bookings</h1>
    <% @bookings.each do |booking| %>
      <div class="card">
        <div class="card-body">
          <h3 class="card-title"><%= booking.bicycle.title %></h3>
          <p>From: <%= booking.start_date %></p>
          <p>To: <%= booking.end_date %></p>
        </div>
      </div>
    <% end %>
</div>
